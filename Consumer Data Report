            const topLTV = [
                {rank: 1, name: 'Hayfever Treatment', ltv: '£408.64', revenue: '£144.15', years: 2.8, customers: 99},
                {rank: 2, name: 'Appreciative Inquiry', ltv: '£394.93', revenue: '£51.78', years: 6.8, customers: 155},
