            populatePracticeList('topRevenuePractices', topRevenue, 'revenue');
            populatePracticeList('topTenurePractices', topTenure, 'tenure');
            populatePracticeList('topLTVPractices', topLTV, 'ltv');
        }

        function populatePracticeList(containerId, practices, type) {
            const container = document.getElementById(containerId);
            container.innerHTML = '';

            practices.forEach(practice => {
                const practiceDiv = document.createElement('div');
                practiceDiv.style.marginBottom = '1rem';
                practiceDiv.style.padding = '1rem';
                practiceDiv.style.backgroundColor = '#f8f9fa';
                practiceDiv.style.borderRadius = '8px';
                practiceDiv.style.borderLeft = '4px solid #3498db';

                let primaryMetric = '';
                if (type === 'revenue') primaryMetric = practice.revenue;
                else if (type === 'tenure') primaryMetric = practice.years + ' years';
                else if (type === 'ltv') primaryMetric = practice.ltv;

                practiceDiv.innerHTML = `
                    <div style="display: flex; align-items: center; margin-bottom: 0.5rem;">
                        <span class="practice-rank">${practice.rank}</span>
                        <span class="practice-name">${practice.name}</span>
                    </div>
                    <div style="font-size: 1.1rem; font-weight: 600; color: #2c3e50; margin-bottom: 0.5rem;">
                        ${primaryMetric}
                    </div>
                    <div class="practice-metrics">
                        <div class="practice-metric">
                            <span>Customers:</span>
                            <span>${practice.customers}</span>
                        </div>
                        <div class="practice-metric">
                            <span>LTV:</span>
                            <span>${practice.ltv || practice.ltv}</span>
                        </div>
                    </div>
                `;

                container.appendChild(practiceDiv);
            });
        }
    </script>
</body>
</html>
