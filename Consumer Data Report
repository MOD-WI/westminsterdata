        function populateTopPractices() {
            // Top Revenue Practices
            const topRevenue = [
                {rank: 1, name: 'Prescribing', revenue: '£162.28', customers: 54, years: 2.0, ltv: '£305.60'},
                {rank: 2, name: 'PDO Threads', revenue: '£160.72', customers: 155, years: 2.4, ltv: '£377.86'},
                {rank: 3, name: 'Hyperhydrosis Treatment', revenue: '£156.45', customers: 142, years: 2.1, ltv: '£332.12'},
